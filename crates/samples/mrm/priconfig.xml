<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<resources targetOsVersion="10.0.0" majorVersion="1">
    <packaging>
        <autoResourcePackage qualifier="Language" />
        <autoResourcePackage qualifier="Scale" />
    </packaging>
    <index root="\resources" startIndexAt="\resources.resfiles">
        <default>
            <!--
              Additional qualifier values can be found at 
              https://docs.microsoft.com/uwp/api/windows.applicationmodel.resources.core.resourcecontext.qualifiervalues
            -->
            <qualifier name="Language" value="en;es" />
            <qualifier name="Contrast" value="standard" />
            <qualifier name="Scale" value="100" />
            <qualifier name="HomeRegion" value="001" />
            <qualifier name="TargetSize" value="256" />
            <qualifier name="LayoutDirection" value="LTR" />
            <qualifier name="Theme" value="dark" />
            <qualifier name="Custom" value="" />
        </default>
        <indexer-config type="resjson" initialPath="Resources" />
        <indexer-config type="resfiles" qualifierDelimiter="." />
    </index>
</resources>